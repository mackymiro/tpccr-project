	</xs:attributeGroup>
	<!--
		**********************************************************
		GLOBAL TABLE ELEMENTS
		**********************************************************
		-->
	<!--
		*****************************
		table
		*****************************
		DESCRIPTION: A formal table in a document.
		ATTRIBUTES:
			frame: the border rules that frame the outside of the table
			colsep: the border rules between columns - a value of 1 = displayed; a value of 0 = suppressed
			rowsep: the border rules between rows - a value of 1 = displayed; a value of 0 = suppressed
			pgwide: a value of 0 = the table rendered in the current text flow; a value of 1 = the table rendered across the full text page
		-->
	<xs:element name="table">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="title" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="tgroup" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="frame">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="top"/>
						<xs:enumeration value="bottom"/>
						<xs:enumeration value="topbot"/>
						<xs:enumeration value="all"/>
						<xs:enumeration value="sides"/>
						<xs:enumeration value="none"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="colsep" type="xs:NMTOKEN"/>
			<xs:attribute name="rowsep" type="xs:NMTOKEN"/>
			<xs:attribute name="pgwide" type="xs:NMTOKEN"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		tgroup
		*****************************
		DESCRIPTION: A logically complete portion of a table.
		ATTRIBUTES:
			cols: the number of columns in the table
			colsep: the border rules between columns - a value of 1 = displayed; a value of 0 = suppressed 
				(overrides table value within this tgroup)
			rowsep: the border rules between rows - a value of 1 = displayed; a value of 0 = suppressed 
				(overrides table value within this tgroup)
			align: the horizontal alignment of text (and other elements) within the cells 
		-->
	<xs:element name="tgroup">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="colspec" minOccurs="0" maxOccurs="unbounded"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="thead"/>
						<xs:element ref="tbody" minOccurs="0"/>
					</xs:sequence>
					<xs:element ref="tbody"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="cols" type="xs:NMTOKEN" use="required"/>
			<xs:attribute name="colsep" type="xs:NMTOKEN"/>
			<xs:attribute name="rowsep" type="xs:NMTOKEN"/>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="center"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="char"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		colspec
		*****************************
		DESCRIPTION: The properties assigned to a single column of a table.
		ATTRIBUTES:
			colnum: the number of the column defined by this colspec
			colname: a symbolic name assigned to a column to identify it for later reference within the table markup
			colwidth: the desired width of the relevant column
			colsep: the border rules between columns - a value of 1 = displayed; a value of 0 = suppressed 
				(overrides tgroup value within this column)
			rowsep: the border rules between rows - a value of 1 = displayed; a value of 0 = suppressed 
				(overrides tgroup value within this column)
			align: the horizontal alignment of text (and other elements) within the cells (overrides tgroup value within this column)
			char: the alignment character when the align attribute is set to "char"
			charoff: the percentage of the column's total width that should appear to the left of the first occurance 
				of the character identified in char when the align attribute is set to "char"
		-->
	<xs:element name="colspec">
		<xs:complexType>
			<xs:attribute name="colnum" type="xs:NMTOKEN"/>
			<xs:attribute name="colname" type="xs:NMTOKEN"/>
			<xs:attribute name="colwidth" type="xs:string"/>
			<xs:attribute name="colsep" type="xs:NMTOKEN"/>
			<xs:attribute name="rowsep" type="xs:NMTOKEN"/>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="center"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="char"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="char" type="xs:string"/>
			<xs:attribute name="charoff" type="xs:NMTOKEN"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		thead
		*****************************
		DESCRIPTION: A table header consisting of one or more rows.
		ATTRIBUTES:
			valign: the vertical alignment of text (and other elements) within the cells
		-->
	<xs:element name="thead">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="row" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="valign">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="top"/>
						<xs:enumeration value="middle"/>
						<xs:enumeration value="bottom"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		tbody
		*****************************
		DESCRIPTION: A wrapper for the rows of a table.
		ATTRIBUTES:
			valign: the vertical alignment of text (and other elements) within the cells
		-->
	<xs:element name="tbody">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="row" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="valign">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="top"/>
						<xs:enumeration value="middle"/>
						<xs:enumeration value="bottom"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		row
		*****************************
		DESCRIPTION: A row in a table.
		ATTRIBUTES:
			rowsep: the border rules between rows - a value of 1 = displayed; a value of 0 = suppressed 
			valign: the vertical alignment of text (and other elements) within the cells
		-->
	<xs:element name="row">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="entry" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="rowsep" type="xs:NMTOKEN"/>
			<xs:attribute name="valign">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="top"/>
						<xs:enumeration value="middle"/>
						<xs:enumeration value="bottom"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
