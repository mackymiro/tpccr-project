		COMPONENT: content.docket.number
		*****************************
		DESCRIPTION: Content Docket Number
		-->
	<xs:element name="content.docket.number">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded" minOccurs="1">
				<xs:element ref="alternate.docket.line" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="docket.line" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnote" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="docket.number" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="text.line" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="conditional.material" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="type">
				<xs:simpleType>
					<!-- 12/18/09 Typically level="lead" -->
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="lead"/>
						<xs:enumeration value="related"/>
						<xs:enumeration value="originating"/>
						<xs:enumeration value="member"/>
						<xs:enumeration value="prior"/>
						<xs:enumeration value="request"/>
						<xs:enumeration value="caption"/>
						<xs:enumeration value="reference"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="alternate.docket.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="suppress-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="docket.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="wide-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="suppress-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="center"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="char"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="docket.number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
