	<xs:element name="appeal.description">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="miscellaneous.information" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="name" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="title.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="level">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="alternate"/>
						<xs:enumeration value="secondary"/>
						<xs:enumeration value="primary"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="venue.number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="source">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="sct"/>
						<xs:enumeration value="cta"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="procedural.history.text">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: resume.material
		*****************************
		DESCRIPTION: Wraps material found within an expert's resume 
		-->
	<xs:element name="resume.material">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="address.block"/>
				<xs:element ref="practice"/>
				<xs:element ref="profession"/>
				<xs:element ref="resume.text"/>
			</xs:choice>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="practice">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="profession">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="resume.text">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
				<xs:element ref="address.block"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: sentence 
		*****************************
		-->
	<xs:element name="sentence">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="label.heading" maxOccurs="unbounded"/>
				<xs:element name="sentence.entry" maxOccurs="unbounded">
					<xs:complexType>
						<xs:choice maxOccurs="unbounded">
							<xs:element ref="maximum"/>
							<xs:element ref="minimum"/>
							<xs:element ref="party.name"/>
							<xs:element ref="sentence.date"/>
							<xs:element ref="sentence.description"/>
							<xs:element ref="sentence.location"/>
							<xs:element ref="sentence.number"/>
							<xs:element ref="sentence.total"/>
							<xs:element ref="sentence.type"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="maximum">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="sentence.length"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="minimum">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="sentence.length"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.date">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.description">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.length">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="units">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="years"/>
						<xs:enumeration value="months"/>
						<xs:enumeration value="days"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.location">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.total">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="sentence.type">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: summary
		*****************************
		DESCRIPTION: 
		-->
	<xs:element name="summary">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="content.cause"/>
				<xs:element ref="content.syllabus"/>
				<xs:element ref="content.synopsis"/>
			</xs:choice>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="source" type="xs:string"/>
			<xs:attribute name="status" type="xs:string"/>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="version" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: text.remarks
		*****************************
		-->
	<xs:element name="text.remarks">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="conditional.material"/>
				<xs:element ref="editorial.note"/>
				<xs:element ref="footnote"/>
				<xs:element ref="footnote.begin.text"/>
				<xs:element ref="footnote.end.text"/>
				<xs:element ref="head"/>
				<xs:element ref="text.remarks.body"/>
				<xs:element ref="voting.record"/>
				<xs:element ref="appendix"/>
			</xs:choice>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="text.remarks.body">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
				<xs:element ref="addressee.block"/>
				<xs:element ref="conditional.material"/>
				<xs:element ref="content.attorney.block"/>
				<xs:element ref="content.author"/>
				<xs:element ref="content.judge"/>
				<xs:element ref="date.line"/>
				<xs:element ref="facts"/>
				<xs:element ref="footnote.begin.text"/>
				<xs:element ref="footnote.end.text"/>
				<xs:element ref="footnote.head"/>
				<xs:element ref="footnote.starpage"/>
				<xs:element ref="procedural.history"/>
				<xs:element ref="mandate"/>
				<xs:element ref="section"/>
				<xs:element ref="signature.line"/>
				<xs:element ref="memo"/>
				<xs:element ref="content.headnote"/>
				<xs:element ref="reference.line"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: title.information 
		*****************************
		DESCRIPTION: 
		-->
	<xs:element name="title.information">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="content.citation"/>
				<xs:element ref="address.line"/>
			</xs:choice>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: trial.segment
		*****************************
		DESCRIPTION: A portion of a trial usually identified with header.
		-->
	<xs:element name="trial.segment">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="head"/>
				<xs:element ref="trial.segment.body" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="party">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="defendant"/>
						<xs:enumeration value="plaintiff"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="closing_stmt"/>
						<xs:enumeration value="cross_exam"/>
						<xs:enumeration value="direct_exam"/>
						<xs:enumeration value="jury_charge"/>
						<xs:enumeration value="motion_hearing"/>
						<xs:enumeration value="opening_stmt"/>
						<xs:enumeration value="plea_hearing"/>
						<xs:enumeration value="recross_exam"/>
						<xs:enumeration value="redirect_exam"/>
						<xs:enumeration value="sentence_hearing"/>
						<xs:enumeration value="voir_dire"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="trial.segment.body">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: trial.type
		*****************************
		DESCRIPTION: Added details that describe the type of case
		-->
	<xs:element name="trial.type">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="uri"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: value
		*****************************
		DESCRIPTION: 
		-->
	<xs:element name="value">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="currency.type" minOccurs="0"/>
				<xs:element ref="monetary.amount" minOccurs="0"/>
				<xs:element ref="monetary.range" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="currency.type">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="monetary.amount">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="monetary.range">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="monetary.range.code">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		COMPONENT: voting.record
		*****************************
		DESCRIPTION: 
		-->
	<xs:element name="voting.record">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="judge.line"/>
				<xs:element ref="voting.line"/>
				<xs:element ref="footnote"/>
			</xs:choice>
			<xs:attribute name="uri" type="xs:string"/>
			<xs:attribute ref="xml:lang"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="voting.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
