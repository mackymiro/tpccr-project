			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="disposition.date" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="disposition.line" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
