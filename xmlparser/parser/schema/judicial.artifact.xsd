	</xs:element>
	<!--
		*****************************
		additional.party.name
		*****************************
		DESCRIPTION: Additional party names not included in the caption that may appear within the Opinion or within the Attorney Block
		-->
	<xs:element name="additional.party.name">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		addressee.block
		*****************************
		DESCRIPTION: Also called an "inside address". The address you're sending your letter to along with title and name.
		-->
	<xs:element name="addressee.block">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="address.block"/>
				<xs:element ref="addressee.name"/>
				<xs:element ref="addressee.title"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="addressee.name">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="addressee.title">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		address.block
		*****************************
		DESCRIPTION: A block of address-related elements
		-->
	<xs:element name="address.block">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="address.line" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="address.unknown" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="street.address" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element ref="city" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="state" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="zip" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="city.state" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="level">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="secondary"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		address.line
		*****************************
		-->
	<xs:element name="address.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="suppress-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		address.unknown
		*****************************
		-->
	<xs:element name="address.unknown">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		alias
		*****************************
		DESCRIPTION: Alias Name.
		-->
	<xs:element name="alias">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		argument.body
		*****************************
		-->
	<xs:element name="argument.body">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		artifact.reference
		*****************************
		DESCRIPTION: A reference to another artifact (document) on Westlaw.
		-->
	<xs:element name="artifact.reference">
		<xs:complexType mixed="true">
			<xs:choice>
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
			<xs:attribute name="target-artifact-ID" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		associated.case
		*****************************
		DESCRIPTION: A case associated with current artifact.
		-->
	<xs:element name="associated.case">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="description" minOccurs="0"/>
				<xs:element ref="events.list"/>
			</xs:sequence>
			<xs:attribute name="type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="mdl.member"/>
						<xs:enumeration value="jpml"/>
						<xs:enumeration value="mdl.related"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		award.party
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="award.party">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="refid" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		award.value
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="award.value">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="refid" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		begin.quote
		*****************************
		DESCRIPTION: The beginning quote. This can be supplied programmaticall or manually. There is a required ID/IDREF relationship with the end.quote.
		-->
	<xs:element name="begin.quote">
		<xs:complexType>
			<xs:attribute name="ID" type="xs:ID" use="optional"/>
			<xs:attribute name="refid" type="xs:IDREF" use="optional"/>
			<xs:attribute name="source" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="manual"/>
						<xs:enumeration value="programmatic"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		case.history
		*****************************
		DESCRIPTION: Wrapper for case history blurbs
		-->
	<xs:element name="case.history">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		case.type
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="case.type">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="type" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="general"/>
						<xs:enumeration value="specific"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		cases.reported.title
		*****************************
		DESCRIPTION: Main Title for Cases Reported
		-->
	<xs:element name="cases.reported.title">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		cent.anomaly
		*****************************
		DESCRIPTION: Ampex cent functions that are not recognized
		-->
	<xs:element name="cent.anomaly">
		<xs:complexType>
			<xs:attribute name="arg" type="xs:string"/>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="value" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		char.anomaly
		*****************************
		DESCRIPTION: Characters that are not currently defined as part of the Ampex character set
		-->
	<xs:element name="char.anomaly">
		<xs:complexType>
			<xs:attribute name="arg" type="xs:string" use="required"/>
			<xs:attribute name="id" type="xs:string"/>
			<xs:attribute name="protected" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		city
		*****************************
		DESCRIPTION: Name of city
		-->
	<xs:element name="city">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		closing
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="closing">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		combining.char.anomaly
		*****************************
		DESCRIPTION: Diacritical combining characters that can not be processed, e.g., there is no valid base character in text to combine with.
		-->
	<xs:element name="combining.char.anomaly">
		<xs:complexType>
			<xs:attribute name="arg" type="xs:anySimpleType" use="required"/>
			<xs:attribute name="type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="base_char_error"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="value" type="xs:anySimpleType" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		conditional.text
		*****************************
		DESCRIPTION: Used to indicate that text may appear in one product format (such as print) but not in another (Westlaw)
		-->
	<xs:element name="conditional.text">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="content.type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="custom.pub.material"/>
						<xs:enumeration value="defendant"/>
						<xs:enumeration value="plaintiff"/>
						<xs:enumeration value="title"/>
						<!-- Format of enumerated values must include periods for this attribute -->
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="product.type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="westlaw"/>
						<xs:enumeration value="annotated.set"/>
						<xs:enumeration value="advance.sheet"/>
						<xs:enumeration value="analytical.toc"/>
						<xs:enumeration value="leg.service"/>
						<xs:enumeration value="ne.reporter"/>
						<xs:enumeration value="ohio.official"/>
						<xs:enumeration value="pocket.part"/>
						<xs:enumeration value="print"/>
						<xs:enumeration value="supplement"/>
						<xs:enumeration value="unpublished.opinion.table"/>
						<xs:enumeration value="westlaw.legacy"/>
						<!-- Format of enumerated values must include periods for this attribute -->
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="action" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="suppress"/>
						<xs:enumeration value="include"/>
						<xs:enumeration value="identification"/>
						<xs:enumeration value="propogate"/>
						<xs:enumeration value="add.note"/>
						<xs:enumeration value="percent_q"/>
						<!-- Format of enumerated values must include periods for this attribute -->
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		construed.term.definition
		*****************************
		DESCRIPTION: The definition of a construed term
		-->
	<xs:element name="construed.term.definition">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="refid" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		county
		*****************************
		DESCRIPTION: County.
		-->
	<xs:element name="county">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		court.message
		*****************************
		DESCRIPTION: An editorial message.
		-->
	<xs:element name="court.message">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="suppress-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		court.number
		*****************************
		DESCRIPTION: court-supplied paragraph numbers
		-->
	<xs:element name="court.number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		deleted.text
		*****************************
		DESCRIPTION: Content that is displayed in a certain way (usually strikethrough) to indicate that it has been striken from a document since the previous version of the document
		-->
	<xs:element name="deleted.text">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="style">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="percent_op"/>
						<xs:enumeration value="percent_x"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		description
		*****************************
		DESCRIPTION: The description of an event, cause, casetype, etc.
		-->
	<xs:element name="description">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		disposition.date
		*****************************
		-->
	<xs:element name="disposition.date">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		disposition.line
		*****************************
		-->
	<xs:element name="disposition.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		editorial.message
		*****************************
		DESCRIPTION: An editorial message.
		-->
	<xs:element name="editorial.message">
		<xs:complexType mixed="true">
			<xs:attribute name="code" type="xs:string"/>
			<xs:attribute name="suppress-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		email
		*****************************
		DESCRIPTION: email address.
		-->
	<xs:element name="email">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		embedded.artifact.reference
		*****************************
		DESCRIPTION: Reference to embedded artifacts
		-->
	<xs:element name="embedded.artifact.reference">
		<xs:complexType>
			<xs:attribute name="filename" type="xs:string"/>
			<xs:attribute name="format" type="xs:string"/>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="location" type="xs:string"/>
			<xs:attribute name="target-artifact-ID" type="xs:string"/>
			<xs:attribute name="target-artifact-ID-type" type="xs:string"/>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="mined-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		embedded.figure.reference
		*****************************
		DESCRIPTION: Reference to embedded figures (or placeholder for image/table)
		-->
	<xs:element name="embedded.figure.reference">
		<xs:complexType>
			<xs:attribute name="image-key" type="xs:string"/>
			<xs:attribute name="location" type="xs:string"/>
			<xs:attribute name="parcel" type="xs:string"/>
			<xs:attribute name="placeholder-type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="image"/>
						<xs:enumeration value="table"/>
						<xs:enumeration value="photo"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="photo-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="psize" type="xs:string"/>
			<xs:attribute name="size" type="xs:string"/>
			<xs:attribute name="rendition-type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="online"/>
						<xs:enumeration value="print"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="source-filename" type="xs:string"/>
			<xs:attribute name="target-ID" type="xs:string"/>
			<xs:attribute name="target-type" type="xs:string"/>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="wide-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		encrypted
		*****************************
		DESCRIPTION: mark-up to encrypt sesnitive information such as SSNs
		-->
	<xs:element name="encrypted">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="key" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		end.quote
		*****************************
		DESCRIPTION: The end quote. There is a required ID/IDREF relationship with the begin.quote.
		-->
	<xs:element name="end.quote">
		<xs:complexType>
			<xs:attribute name="ID" type="xs:ID" use="optional"/>
			<xs:attribute name="refid" type="xs:IDREF" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		entity.reference
		*****************************
		DESCRIPTION: A reference to an entity in another document (e.g., judge, attorney, witness, etc.).
		-->
	<xs:element name="entity.reference">
		<xs:complexType mixed="true">
			<xs:choice>
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
			<xs:attribute name="name" type="xs:string"/>
			<xs:attribute name="role" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		entry
		*****************************
		DESCRIPTION: A single cell in a table.
		ATTRIBUTES:
			colname: the symbolic name assigned to a column in the colspec element
			namest: the name (defined in a colspec) of the leftmost column of a span
			nameend:  the name (defined in a colspec) of the rightmost column of a span
			morerows: a verticl span created by indicating how many more rows, in addition to the current row, this entry is to occupy
			colsep: the border rules between columns - a value of 1 = displayed; a value of 0 = suppressed
				(overrides tgroup value within this column)
			rowsep: the border rules between rows - a value of 1 = displayed; a value of 0 = suppressed
				(overrides tgroup value within this column)
			align: the horizontal alignment of text within the cells (overrides tgroup value within this column)
			char: the alignment character when the align attribute is set to "char"
			charoff: the percentage of the column's total width that should appear to the left of the first occurance
				of the character identified in char when the align attribute is set to "char"
			valign: the vertical alignment of text (and other elements) within the cells
		-->
	<xs:element name="entry">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="head"/>
				<xs:element ref="para"/>
			</xs:choice>
			<xs:attributeGroup ref="entry-attlist"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		error.message
		*****************************
		DESCRIPTION: An error messages.
		-->
	<xs:element name="error.message">
		<xs:complexType mixed="true">
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="citation"/>
						<xs:enumeration value="content"/>
						<xs:enumeration value="copyright"/>
						<xs:enumeration value="court"/>
						<xs:enumeration value="opinion"/>
						<xs:enumeration value="reporter"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="code" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		fax
		*****************************
		DESCRIPTION: Fax number.
		-->
	<xs:element name="fax">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		figure
		*****************************
		DESCRIPTION: Wrapper for an embedded.figure.reference (image link or placeholder for image/table)
		-->
	<xs:element name="figure">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="embedded.figure.reference"/>
				<xs:element ref="footnote"/>
				<xs:element ref="keywords"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		footnote.begin.text
		*****************************
		DESCRIPTION: Displayed text, e.g., "FOOTNOTE(s).", that specifically marks the beginning of footnotes.
		-->
	<xs:element name="footnote.begin.text">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		footnote.end.text
		*****************************
		DESCRIPTION: Displayed text, e.g., "End of Footnote(s).", indicating where footnotes end.
		-->
	<xs:element name="footnote.end.text">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		footnote.head
		*****************************
		DESCRIPTION: heading for footnote material.
		-->
	<xs:element name="footnote.head">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="headtext"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		footnote
		footnote.body
		*****************************
		DESCRIPTION: A footnote
		-->
	<xs:element name="footnote">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="footnote.body">
					<xs:complexType>
						<xs:choice maxOccurs="unbounded">
							<xs:group ref="paralevel"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
			<xs:attribute name="refid" type="xs:IDREF"/>
			<xs:attribute name="designator">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="symbol"/>
						<xs:enumeration value="super"/>
						<xs:enumeration value="percent_r"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		footnote.reference
		*****************************
		DESCRIPTION: This element is used to identify references to a footnote that is located at another point in the text.  For example, the number, asterisk, letter, etc., that is used to reference the note.
		-->
	<xs:element name="footnote.reference">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="refid" type="xs:IDREF"/>
			<xs:attribute name="style" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="percent_n"/>
						<xs:enumeration value="percent_nd"/>
						<xs:enumeration value="percent_nl"/>
						<xs:enumeration value="percent_r"/>
						<xs:enumeration value="percent_rd"/>
						<xs:enumeration value="percent_rl"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		footnote.starpage
		*****************************
		DESCRIPTION: non-displayed starpage, that precedes the beginning of footnotesin NY Officials
		-->
	<xs:element name="footnote.starpage">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		fts.number
		*****************************
		DESCRIPTION: Federal Telephone Service number
		-->
	<xs:element name="fts.number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		head
		headtext
		*****************************
		DESCRIPTION: A general purpose semantic heading.
		-->
	<xs:element name="head">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="head"/>
				<xs:element ref="headtext"/>
				<xs:element ref="error.message"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="suppress-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="headtext">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="center"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="contd-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="first-line" type="xs:string"/>
			<xs:attribute name="format">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ital"/>
						<xs:enumeration value="caps"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="level" type="xs:string"/>
			<xs:attribute name="turnover" type="xs:string"/>
			<xs:attribute name="wide-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="starpage-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		internal.reference
		*****************************
		DESCRIPTION: internal reference number.
		-->
	<xs:element name="internal.reference">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="refid" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		introduction
		*****************************
		DESCRIPTION: internal reference number.
		-->
	<xs:element name="introduction">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="head"/>
				<xs:element ref="para"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		judge
		*****************************
		DESCRIPTION: Inline instance of a judge.
		-->
	<xs:element name="judge">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		judge.last.name
		*****************************
		DESCRIPTION: Inline instance of a judges last name - needed for Ohio RHS.
		-->
	<xs:element name="judge.last.name">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		judge.title
		*****************************
		DESCRIPTION: Inline instance of a judge title information.  Needed for Ohio RHS.
		-->
	<xs:element name="judge.title">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		keywords
		*****************************
		DESCRIPTION: a list of key words and phrases associated with a figure.
		-->
	<xs:element name="keywords">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		label.designator
		*****************************
		DESCRIPTION: the label designator for footnotes.
		-->
	<xs:element name="label.designator">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="refid" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		label.heading
		*****************************
		DESCRIPTION: a label acting as a column heading for a tabular-style layout.
		-->
	<xs:element name="label.heading">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		letter.line
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="letter.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		letter.head
		*****************************
		-->
	<xs:element name="letter.head">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		letter.subject
		*****************************
		-->
	<xs:element name="letter.subject">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		license.number.block
		*****************************
		DESCRIPTION: 
		-->
	<xs:element name="license.number.block">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="license.number">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
						<xs:attribute name="label" type="xs:string"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		location
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="location">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="type" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="dept"/>
						<xs:enumeration value="docket"/>
						<xs:enumeration value="room"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		mandate
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="mandate">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="para"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		mandate.text
		*****************************
		-->
	<xs:element name="mandate.text">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		manual.add
		*****************************
		-->
	<xs:element name="manual.add">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		manual.change
		*****************************
		-->
	<xs:element name="manual.change">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		media.anchor
		*****************************
		DESCRIPTION: media anchor.
		-->
	<xs:element name="media.anchor">
		<xs:complexType>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
			<xs:attribute name="target-artifact-id" type="xs:string"/>
			<xs:attribute name="time-stamp" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		miscellaneous.information
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="miscellaneous.information">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		name
		*****************************
		DESCRIPTION: Name.
		-->
	<xs:element name="name">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="variable-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		name.normalized
		*****************************
		DESCRIPTION: Normalized name.
		-->
	<xs:element name="name.normalized">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		offense
		*****************************
		DESCRIPTION: The offense charged against a suspect.
		-->
	<xs:element name="offense">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="count">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element name="count.description">
								<xs:complexType mixed="true">
									<xs:choice minOccurs="0" maxOccurs="unbounded">
										<xs:group ref="inline.text"/>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element ref="disposition"/>
							<xs:element name="offense.level">
								<xs:complexType mixed="true">
									<xs:choice minOccurs="0" maxOccurs="unbounded">
										<xs:group ref="inline.text"/>
									</xs:choice>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element ref="label.heading"/>
			</xs:choice>
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="complaints"/>
						<xs:enumeration value="pending"/>
						<xs:enumeration value="terminated"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		page.indicator
		*****************************
		DESCRIPTION: page.indicator
		-->
	<xs:element name="page.indicator">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		para
		*****************************
		DESCRIPTION: General text paragraph.
		-->
	<xs:element name="para">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="figure"/>
				<xs:element ref="footnote"/>
				<xs:element ref="label.designator"/>
				<xs:element ref="media.anchor"/>
				<xs:element ref="para"/>
				<xs:element ref="paratext"/>
				<xs:element ref="paratext.subphrase"/>
				<xs:element ref="ruled.line"/>
				<xs:element ref="text.line"/>
				<xs:element ref="text.omission"/>
				<xs:element ref="tbl"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="headnote-ref-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		paratext
		*****************************
		DESCRIPTION: General text paragraph.
		-->
	<xs:element name="paratext">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="center"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="contd-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="first-line" type="xs:string"/>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="level" type="xs:string"/>
			<xs:attribute name="style">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="block"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="turnover" type="xs:string"/>
			<xs:attribute name="variable-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="wide-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		paratext.subphrase
		*****************************
		DESCRIPTION: Subparagraph phrase paragraph.
		-->
	<xs:element name="paratext.subphrase">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID"/>
			<xs:attribute name="level" type="xs:string"/>
			<xs:attribute name="contd-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		party.name
		*****************************
		DESCRIPTION: General text paragraph.
		-->
	<xs:element name="party.name">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		participant
		*****************************
		DESCRIPTION: Participant.
		-->
	<xs:element name="participant">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="role">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="name">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		pasteup
		*****************************
		DESCRIPTION: where manual addition to mark-up gets added for print products
		-->
	<xs:element name="pasteup">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		pdc.number
		*****************************
		DESCRIPTION: public domain cite numbers
		-->
	<xs:element name="pdc.number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		percent.anomaly
		*****************************
		DESCRIPTION: Ampex pencent functions that are not recognized
		-->
	<xs:element name="percent.anomaly">
		<xs:complexType>
			<xs:attribute name="arg" type="xs:string"/>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="value" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		phone
		*****************************
		DESCRIPTION: Phone number.
		-->
	<xs:element name="phone">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		pinpoint.anchor
		*****************************
		DESCRIPTION: pinpoint anchor
		-->
	<xs:element name="pinpoint.anchor">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
			<xs:attribute name="product-citation-id" type="xs:string"/>
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="starpage_westlaw"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		private
		*****************************
		DESCRIPTION: A wrapper for material considered "private".
		-->
	<xs:element name="private">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="original" type="xs:string" use="required"/>
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ARN"/>
						<xs:enumeration value="DL"/>
						<xs:enumeration value="DOB"/>
						<xs:enumeration value="FAN"/>
						<xs:enumeration value="SSN"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		prog.add
		*****************************
		DESCRIPTION: A wrapper for material added by program.
		-->
	<xs:element name="prog.add">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		prog.change
		*****************************
		DESCRIPTION: A wrapper for material changed by a program.
		-->
	<xs:element name="prog.change">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="explicit-replace-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="true"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		reference.line
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="reference.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="reference-type" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		relationship
		*****************************
		-->
	<xs:element name="relationship">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		ruled.line
		*****************************
		DESCRIPTION: Paragraph which contains a line.
		-->
	<xs:element name="ruled.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="center"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		salutation
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="salutation">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		section
		*****************************
		-->
	<xs:element name="section">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="conditional.material"/>
				<xs:element ref="error.message"/>
				<xs:element ref="footnote"/>
				<xs:element ref="head"/>
				<xs:element ref="introduction"/>
				<xs:element ref="para"/>
				<xs:element name="reference.head">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element ref="section.body"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		section.body
		*****************************
		-->
	<xs:element name="section.body">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="paralevel"/>
				<xs:element ref="conditional.material"/>
				<xs:element ref="letter.head"/>
				<xs:element ref="letter.subject"/>
				<xs:element ref="signature.line"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		separator.line
		*****************************
		DESCRIPTION: Line of Stars that acts as a separator between lines.
		-->
	<xs:element name="separator.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		signature.line
		*****************************
		-->
	<xs:element name="signature.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		ssn.block
		*****************************
		DESCRIPTION: Social Security number and corresponding SSN fragment
		-->
	<xs:element name="ssn.block">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ssn" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
							<xs:element ref="encrypted"/>
						</xs:choice>
						<xs:attribute name="srch" type="xs:string"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="ssn.frag" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
							<xs:element ref="encrypted"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		starpage
		*****************************
		DESCRIPTION: A starpage reference
		-->
	<xs:element name="starpage">
		<xs:complexType>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		state.abbr
		*****************************
		DESCRIPTION: State abbreviation
		-->
	<xs:element name="state.abbr">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="style">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="cent_b"/>
						<xs:enumeration value="cent_h"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		status
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="status">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		subject
		*****************************
		DESCRIPTION:
		-->
	<xs:element name="subject">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		table.footnote.reference
		*****************************
		DESCRIPTION: This element is used to identify references to a footnote that are within a table.  For example, the number, asterisk, letter, etc., that is used to reference the note.
		-->
	<xs:element name="table.footnote.reference">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID" use="optional"/>
			<xs:attribute name="refid" type="xs:IDREF"/>
			<xs:attribute name="style" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="percent_n"/>
						<xs:enumeration value="percent_nd"/>
						<xs:enumeration value="percent_nl"/>
						<xs:enumeration value="percent_r"/>
						<xs:enumeration value="percent_rd"/>
						<xs:enumeration value="percent_rl"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		tax.id
		*****************************
		DESCRIPTION: Tax identifier.
		-->
	<xs:element name="tax.id">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		tbl
		*****************************
		DESCRIPTION: A West wrapper of CALS tables that allows the assignment of a Novus GUID
			as well as additional elements outside of the standard CALS model
		-->
	<xs:element name="tbl">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="footnote"/>
				<xs:element ref="head"/>
				<xs:element ref="para"/>
				<xs:element ref="table"/>
			</xs:choice>
			<xs:attribute name="ID" type="xs:ID" use="required"/>
			<xs:attribute name="maxsize" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		text.line
		*****************************
		DESCRIPTION: Alternate text paragraph. Often different leading.
		-->
	<xs:element name="text.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="left"/>
						<xs:enumeration value="right"/>
						<xs:enumeration value="justify"/>
						<xs:enumeration value="center"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		text.omission
		*****************************
		DESCRIPTION: Alternate text paragraph for a Line of Stars that represents omitted text.
		-->
	<xs:element name="text.omission">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		title
		*****************************
		DESCRIPTION: table title
		NOTE: the element name "title" is reserved strictly for use within the cals.table model. It should not be used
			for any sort of content title within the document.
		-->
	<xs:element name="title">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		time
		*****************************
		DESCRIPTION: Time of day
		-->
	<xs:element name="time">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="type" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="misc"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		two.cases.term
		*****************************
		DESCRIPTION: "Two Cases" terminology
		-->
	<xs:element name="two.cases.term">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		undefined.content
		*****************************
		DESCRIPTION: Undefined content.
		-->
	<xs:element name="undefined.content">
		<xs:complexType mixed="true"/>
	</xs:element>
	<!--
		*****************************
		unknown
		*****************************
		DESCRIPTION: A generic tag that is used to indicate to the data owners that the element must be investigated and correct mark-up applied
		-->
	<xs:element name="unknown">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		url
		*****************************
		DESCRIPTION: Uniform Resource Locator.
		-->
	<xs:element name="url">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		vendor.extract
		*****************************
		DESCRIPTION: editorally added material within data being sent to an outside vendor
		-->
	<xs:element name="vendor.extract">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="override">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="exclude"/>
						<xs:enumeration value="include"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		venue.name
		*****************************
		-->
	<xs:element name="venue.name">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="label" type="xs:string"/>
			<xs:attribute name="suppress-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="display-flag">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="y"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		vetoed.text
		*****************************
		DESCRIPTION: Vetoed text in Federal Reporter
		-->
	<xs:element name="vetoed.text">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		wip.flag
		*****************************
		DESCRIPTION: Used to delineate whited out material in some Caselaw documents.
		-->
	<xs:element name="wip.flag">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="legacy.markup" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		wl.html
		*****************************
		DESCRIPTION: Westlaw online HTML
		-->
	<xs:element name="wl.html">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
