<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="http://www.thomsonreuters.com/legal/judicialdocs" version="??" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.thomsonreuters.com/legal/judicialdocs">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd"/>
	<!--
		**********************************************************
		judicial.artifact.xsd
		v6.5
		12/10/2013
