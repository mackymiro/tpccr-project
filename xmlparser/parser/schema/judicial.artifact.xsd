		-->
	<xs:element name="embedded.artifact">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="appendix"/>
				<xs:element ref="judicial.artifact"/>
				<xs:sequence>
					<xs:element name="alt.text" minOccurs="0">
						<xs:complexType mixed="true"/>
