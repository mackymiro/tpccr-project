		Proprietary and Confidential information of TRGR. Disclosure, Use or Reproduction without the written authorization of TRGR is prohibited
		-->
	<!--
		********************************************************** 
		NOVUS ELEMENTS
		********************************************************** 
		DESCRIPTION: Elements required to load documents to a Novus collection
		-->
	<!--
		***************************** 
		n-load
		***************************** 
		DESCRIPTION: the root element of a Novus document
		-->
	<xs:element name="n-load">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="n-document" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="dataid" type="xs:string"/>
			<xs:attribute name="verifycollection" type="xs:string"/>
			<xs:attribute name="notify" type="xs:string"/>
			<xs:attribute name="lang" type="xs:string"/>
			<xs:attribute name="country" type="xs:string"/>
			<xs:attribute name="loadcontenttype" default="ALL">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ALL"/>
						<xs:enumeration value="METADOC"/>
						<xs:enumeration value="SECTIONAL"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		n-document
		***************************** 
		DESCRIPTION: a Novus document (i.e., an XML file loaded to a DOC collection)
		-->
	<xs:element name="n-document">
		<xs:complexType>
			<xs:choice minOccurs="0">
				<xs:sequence>
					<xs:element ref="n-metadata" minOccurs="0"/>
					<xs:element ref="n-docbody"/>
				</xs:sequence>
			</xs:choice>
			<xs:attribute name="guid" type="valid.uuid"/>
			<xs:attribute name="control" default="ADD">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ADD"/>
						<xs:enumeration value="DEL"/>
						<xs:enumeration value="REINDEX"/>
						<xs:enumeration value="PARTIAL"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		**********************************************************
		TYPOGRAPHIC SIMPLE TYPES
		**********************************************************
		-->
	<!--
		***************************** 
		alignment
		***************************** 
		DESCRIPTION: alignment values (left, right, center, justify) for quadding attributes
		-->
	<xs:simpleType name="alignment">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="l"/>
			<xs:enumeration value="r"/>
			<xs:enumeration value="c"/>
			<xs:enumeration value="j"/>
		</xs:restriction>
	</xs:simpleType>
	<!--
		***************************** 
		rendering.properties
		***************************** 
		DESCRIPTION: rendering properties to be stored or restored
		-->
	<xs:simpleType name="rendering.properties">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="font"/>
			<xs:enumeration value="pointsize"/>
			<xs:enumeration value="setwidth"/>
			<xs:enumeration value="leading"/>
			<xs:enumeration value="measure"/>
		</xs:restriction>
	</xs:simpleType>
	<!--
		***************************** 
		rule.positions
		***************************** 
		DESCRIPTION: position choice for where rule is to be rendered
		-->
	<xs:simpleType name="rule.positions">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="baseline"/>
			<xs:enumeration value="midline"/>
			<xs:enumeration value="ascender"/>
			<xs:enumeration value="descender"/>
		</xs:restriction>
	</xs:simpleType>
	<!--
		********************************************************** 
		TYPOGRAPHIC ATTRIBUTE GROUPS
		**********************************************************
		-->
	<!--
		***************************** 
		typo-group
		*****************************
		DESCRIPTION: the typographic properties that can be applied to character text
		-->
	<xs:attributeGroup name="typo-group">
		<xs:attribute name="fontname" type="xs:string" use="optional"/>
		<xs:attribute name="pointsize" type="xs:decimal" use="optional"/>
		<xs:attribute name="setwidth" type="xs:string" use="optional"/>
		<xs:attribute name="leading" type="xs:string" use="optional"/>
		<xs:attribute name="measure" type="xs:string" use="optional"/>
		<xs:attribute name="relative-size" type="xs:string" use="optional"/>
		<xs:attribute name="vertical-offset" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<!--
		********************************************************** 
		TYPOGRAPHIC ELEMENT GROUPS
		**********************************************************
		-->
	<!--
		*****************************
		inline.text 
		*****************************
		DESCRIPTION: the typographic properties that can be applied to character text
		-->
	<xs:group name="inline.text">
		<xs:choice>
			<xs:group ref="typographic.set"/>
			<xs:group ref="inline.content"/>
		</xs:choice>
	</xs:group>
	<!--
		***************************** 
		typographic.set
		*****************************
		DESCRIPTION: contains all typographical elements and is owned by all of the elements it contains
		-->
	<xs:group name="typographic.set">
		<xs:choice>
			<xs:element ref="bold"/>
			<xs:element ref="charfill"/>
			<xs:element ref="crosshatch"/>
			<xs:element ref="csc"/>
			<xs:element ref="dbluscore"/>
			<xs:element ref="endline"/>
			<xs:element ref="eol"/>
			<xs:element ref="font"/>
			<xs:element ref="fraction"/>
			<xs:element ref="hide"/>
			<xs:element ref="hsi"/>
			<xs:element ref="ital"/>
			<xs:element ref="justified.line"/>
			<xs:element ref="justifying.space"/>
			<xs:element ref="leader"/>
			<xs:element ref="overunderbar"/>
			<xs:element ref="predefined.charstring"/>
			<xs:element ref="restore"/>
			<xs:element ref="set.line.indent"/>
			<xs:element ref="store"/>
			<xs:element ref="sub"/>
			<xs:element ref="super"/>
			<xs:element ref="typesize"/>
			<xs:element ref="typo.format"/>
			<xs:element ref="uc"/>
			<xs:element ref="underscore"/>
		</xs:choice>
	</xs:group>
	<!--
		********************************************************** 
		TYPOGRAPHIC ELEMENTS
		**********************************************************
		-->
	<!--
		***************************** 
		bold
		***************************** 
		DESCRIPTION: Bold font
		-->
	<xs:element name="bold">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		charfill
		***************************** 
		DESCRIPTION: Character fill (fixed leader)
		ATTRIBUTES:
			numchar: The number of repetitions of fill material
			length: The length
			position: The position of rendering (baseline, midline, ascender, descender)
		-->
	<xs:element name="charfill">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="numchar" type="xs:integer" use="optional"/>
			<xs:attribute name="length" type="xs:string" use="optional"/>
			<xs:attribute name="position" type="rule.positions" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		crosshatch
		***************************** 
		DESCRIPTION: Crosshatch
		-->
	<xs:element name="crosshatch">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		csc
		***************************** 
		DESCRIPTION: Caps/small caps
		-->
	<xs:element name="csc">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		dbluscore
		***************************** 
		DESCRIPTION: Double underscore
		-->
	<xs:element name="dbluscore">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		endline
		***************************** 
		DESCRIPTION: Line termination and quadding
		ATTRIBUTES:
			quadding: alignment values (left, right, center, justify)
		-->
	<xs:element name="endline">
		<xs:complexType>
			<xs:attribute name="quadding" type="alignment" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		eol
		***************************** 
		DESCRIPTION: End-of-print-line marker
		ATTRIBUTES:
			source: product origin
		-->
	<xs:element name="eol">
		<xs:complexType>
			<xs:attribute name="source" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		font
		***************************** 
		DESCRIPTION: Select font
		ATTRIBUTES:
			fontname: the name of the font
		-->
	<xs:element name="font">
		<xs:complexType>
			<xs:attribute name="fontname" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		fraction
		***************************** 
		DESCRIPTION: Fraction
		The child element "normalized.fraction" is used for online indexing
		ATTRIBUTES:
			style: type of fraction (shilling, builtup)
		-->
	<xs:element name="fraction">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="numerator">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="denominator">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="style">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="shilling"/>
						<xs:enumeration value="builtup"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		hide
		***************************** 
		DESCRIPTION: Set tranparently; hide text
		-->
	<xs:element name="hide">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		hsi
		***************************** 
		DESCRIPTION: Horizontal space immediate
		ATTRIBUTES:
			value: value of horizontal space
		-->
	<xs:element name="hsi">
		<xs:complexType>
			<xs:attribute name="value" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		ital
		***************************** 
		DESCRIPTION: Italic
		-->
	<xs:element name="ital">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		justified.line
		***************************** 
		DESCRIPTION: 
		-->
	<xs:element name="justified.line">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="quadding" type="alignment" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		justifying.space
		***************************** 
		DESCRIPTION: 
		-->
	<xs:element name="justifying.space">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		leader
		***************************** 
		DESCRIPTION: Leader (leadered material set flush right with specified leadering)
		-->
	<xs:element name="leader">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="leaderchar" type="xs:string"/>
				<xs:element name="leadercon" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:group ref="inline.text"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="source" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="pctw"/>
						<xs:enumeration value="dtbl"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		overunderbar
		***************************** 
		DESCRIPTION: Over- and underscore
		-->
	<xs:element name="overunderbar">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		predefined.charstring
		***************************** 
		DESCRIPTION: Predefined character string
		ATTRIBUTES:
			charcode: character code
		-->
	<xs:element name="predefined.charstring">
		<xs:complexType>
			<xs:attribute name="charcode" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		restore
		***************************** 
		DESCRIPTION: property to be restored: (font, pointsize, setwidth, leading, measure)
		-->
	<xs:element name="restore">
		<xs:complexType>
			<xs:attribute name="property" type="rendering.properties" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		set.line.indent
		***************************** 
		DESCRIPTION: Set line indents
		ATTRIBUTES:
			li: the left indent of the first line
			lroi: the left indent of the first line, and the left indent of runover lines (all lines after the first)
			ri: the right indent of the first line
			rroi: the right indent of the first line, and the right indent of runover lines
		-->
	<xs:element name="set.line.indent">
		<xs:complexType>
			<xs:attribute name="li" type="xs:string" use="optional"/>
			<xs:attribute name="lroi" type="xs:string" use="optional"/>
			<xs:attribute name="ri" type="xs:string" use="optional"/>
			<xs:attribute name="rroi" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		store
		***************************** 
		DESCRIPTION: Store typesetting parameters
		ATTRIBUTES:
			property: values are: font, pointsize, setwidth, leading, measure
		-->
	<xs:element name="store">
		<xs:complexType>
			<xs:attribute name="property" type="rendering.properties" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		sub
		***************************** 
		DESCRIPTION: Subscript
		ATTRIBUTES:
			style: the percent ampex codes
		-->
	<xs:element name="sub">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="style" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="percent_l"/>
						<xs:enumeration value="percent_ld"/>
						<xs:enumeration value="percent_ll"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		super
		***************************** 
		DESCRIPTION: Superscript
		ATTRIBUTES:
			style: the percent ampex codes
		-->
	<xs:element name="super">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="style" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="percent_g"/>
						<xs:enumeration value="percent_gd"/>
						<xs:enumeration value="percent_gl"/>
						<xs:enumeration value="percent_r"/>
						<xs:enumeration value="percent_rd"/>
						<xs:enumeration value="percent_rl"/>
						<xs:enumeration value="percent_re"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		typesize
		***************************** 
		DESCRIPTION: Select type size (pointsize and setwidth)
		ATTRIBUTES:
			pointsize: units of measure are points
			setwidth: units of measure are points
			relative-size: ??
		-->
	<xs:element name="typesize">
		<xs:complexType>
			<xs:attribute name="pointsize" type="xs:decimal" use="optional"/>
			<xs:attribute name="setwidth" type="xs:string" use="optional"/>
			<xs:attribute name="relative-size" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		typo.format
		***************************** 
		DESCRIPTION: 
		ATTRIBUTES:
			fontname: Font name
			pointsize: units of measure are points
			setwidth: units of measure are points
			leading: amount of leading
			measure: distance between the left and right margins of the rendering field
			relative-size: ??
			vertical-offset: ??
		-->
	<xs:element name="typo.format">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attributeGroup ref="typo-group"/>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		UC
		***************************** 
		DESCRIPTION: Upper case (all caps)
		-->
	<xs:element name="uc">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		***************************** 
		underscore
		***************************** 
		DESCRIPTION: Underscore
		-->
	<xs:element name="underscore">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!--
		********************************************************** 
		LINK MARKUP ELEMENTS
		********************************************************** 
		DESCRIPTION: Elements used to link from one document to another
		-->
	<!--
		*****************************
		cite.query
		*****************************
		DESCRIPTION: A resolved form of a cite instance, using attributes to pass to the Westlaw FIND process for link resolution.
		-->
	<xs:element name="cite.query">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="w-src-number" type="xs:string"/>
			<xs:attribute name="w-seq-number" type="xs:string"/>
			<xs:attribute name="w-serial-number" type="xs:string"/>
			<xs:attribute name="w-ref-type" type="xs:string"/>
			<xs:attribute name="w-normalized-cite" type="xs:string"/>
			<xs:attribute name="w-pub-number" type="xs:string"/>
			<xs:attribute name="w-pinpoint-page" type="xs:string"/>
			<xs:attribute name="w-docfamily-uuid" type="xs:string"/>
			<xs:attribute name="w-target-preference" type="xs:string"/>
			<xs:attribute name="format" type="xs:string" fixed="cent_r"/>
			<xs:attribute name="ID" type="valid.uuid"/>
			<xs:attribute name="manual-edit" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="true"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="manual-verification-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="true"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="acceptable-error-flag" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="true"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		entity.link
		*****************************
		DESCRIPTION: Used to mark an unverified entity reference.
		-->
	<xs:element name="entity.link">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="entity.id" type="xs:string" use="required"/>
			<xs:attribute name="etype" type="xs:string" use="required"/>
			<xs:attribute name="vrule" type="xs:string"/>
			<xs:attribute name="pv" type="xs:string"/>
			<xs:attribute name="rel.id" type="xs:ID"/>
			<xs:attribute name="tlrclass" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="entitylink"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		entity
		*****************************
		DESCRIPTION: Used to mark an unverified entity reference.
		-->
	<xs:element name="entity">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="inline.text"/>
			</xs:choice>
			<xs:attribute name="etype" type="xs:string" use="required"/>
			<xs:attribute name="nvreason" type="xs:string"/>
			<xs:attribute name="rel.id" type="xs:ID"/>
			<xs:attribute name="tlrclass" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="entity"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--
		*****************************
		dockets.link
		*****************************
		DESCRIPTION: Used for older Dockets-style links.
		-->
	<xs:element name="dockets.link">
		<xs:complexType mixed="true">
