<!ENTITY % ISOLAT1 PUBLIC "ISO 8879:1986//ENTITIES Added Latin 1//EN">
<!ENTITY % ISOLAT2 PUBLIC "ISO 8879:1986//ENTITIES Added Latin 2//EN">
<!ENTITY % ISONUM  PUBLIC "ISO 8879:1986//ENTITIES Numeric and Special Graphic//EN">
<!ENTITY % ISOPUB  PUBLIC "ISO 8879:1986//ENTITIES Publishing//EN">
<!ENTITY % ISOTECH PUBLIC "ISO 8879:1986//ENTITIES General Technical//EN">
<!ENTITY % ISOAMSB PUBLIC "ISO 8879:1986//ENTITIES Added Math Symbols: Binary Operators//EN">

-->
<!-- PUT THE FOLLOWING TWO DEFINITIONS IN COMMENT IF THE PROGRAM FOR THE VISUALISATION OF THE DTD (EX: NEAR AND FAR) DO NOT FIND THE ENTITIES -->
<!--
<!ENTITY % ENTCAR1 PUBLIC "-//Carswell//ENTITIES Copyright Page Text//EN">
<!ENTITY % ENTCAR2 PUBLIC "-//Carswell//ENTITIES Carswell//EN">

%ISOLAT1;
%ISOLAT2;
%ISONUM;
%ISOPUB;
%ISOTECH;
%ISOAMSB;
-->

<!-- PUT THE FOLLOWING TWO DEFINITIONS IN COMMENT IF THE PROGRAM FOR THE VISUALISATION OF THE DTD (EX: NEAR AND FAR) DO NOT FIND THE ENTITIES -->
<!--
%ENTCAR1;
%ENTCAR2;
-->


<!ENTITY % iso-ents SYSTEM "file:/camp/dtd/content/iso-ents.ent">
%iso-ents;
<!ENTITY % xcarswell-symbol SYSTEM "file:/camp/dtd/content/xcarswell-symbol.ent">
%xcarswell-symbol;

<!ENTITY % everyshort "(DEBUTBLOC | FINBLOC)*">

<!ENTITY % everylong "(DEBUTBLOC | EDITNOTE | FINBLOC | QUOTE | PATCH | FOOTNOTE)*">



<!--PARAMETER ENTITY: INLINE TEXT ELEMENTS.
    TYPOGRAPHIC CONVENTIONS.
    CROSS-REFERENCES.
    TEXT EXCLUSION/INCLUSION.
    MODIFICATION MARKER.
-->

<!ENTITY % LINETXT "#PCDATA | BOLD | SHADING | EM |  STRIKE | UNDER | SUB | SUP |
                     TEXTFRAC | STATREF | TERM | TERMREF | SEE |
                     SEEALSO | EX | TEXT | MODIFY | EXPRDIV | EXPRERR" >

<!--PARAMETER ENTITY: IN-LINE TEXT ELEMENTS + PARAGRAPH-BREAKING ELEMENTS.
    TYPOGRAPHIC CONVENTIONS.
    CROSS-REFERENCES.
    TEXT EXCLUSION/INCLUSION.
    MODIFICATION MARKER.
    FORMULAS.
    GRAPHIC PLACEHOLDERS.
    ORDERED AND UNORDERED LISTS.
    QUOTES AND EXCERPTS.
    TABLES.
    "WORDS AND PHRASES" TERM PARAGRAPHS.
-->


<!ENTITY % PARATXT "#PCDATA | BOLD | SHADING | EM |  STRIKE | UNDER | SUB | SUP |
                     TEXTFRAC | EXPRDIV | EXPRERR |
                     STATREF | TERM | TERMREF |
                     EX | TEXT |
                     MODIFY |
                     DF |
                     GRAPHIC |
                     ITEM | LINE |
                     ENDNOTE |
                     TABLE | QUOTE" >


<!--PARAMETER ENTITY: FORMULA AND FRACTION ELEMENTS.
    TYPOGRAPHIC ELEMENTS.
    FRACTION.
    FENCED FRACTION.
-->
<!ENTITY % FRACTXT "#PCDATA | SUP | SUB | FRAC | FENCE" >


<!-- A LEGISLATIVE DOCUMENT CAN CONTAIN A LAW OR A REGULATION  -->
<!ELEMENT ROOT  (ACT|REG|ORDER|EXTRACT|PATCH)+  > <!--PLM 28/07/2003-->


<!-- ELEMENT : LAW  -->
<!ELEMENT ACT  (%everyshort; , (IDENTIFICATION, LEGIDDOC?, PREAMBLE?, (DIVHEADING|FLOATHEADING|SEC)*,
                    EXTERNSCHEDULE*, CONTAINR*, NOTEFINLR?) , %everyshort;) >

<!-- ABBREV IS A UNIQUE IDENTIFIER FOR THE LAW
     EX: ABBREV = "LRQ[L-5.2][FR]"
-->
<!ATTLIST ACT
               ABBREV    CDATA     #IMPLIED
               LANGUAGE  (FR|EN)   #IMPLIED>

<!-- ELEMENT : REGULATION -->
<!ELEMENT REG  (IDENTIFICATION, LEGIDDOC? ,PREAMBLE?, (DIVHEADING|FLOATHEADING|SEC)*,
                    EXTERNSCHEDULE*, CONTAINR*, NOTEFINLR?) >

<!-- ABBREV IS A UNIQUE IDENTIFIER FOR THE REGULATION
     EX: ABBREV = "RRQ[L-5.2R1][FR]" [TO BE DETERMINED....]
-->

<!ATTLIST REG
               TYPE      (rules) #IMPLIED
               ABBREV    CDATA     #IMPLIED
               LABEL     CDATA   #IMPLIED
               PROCLAIM  (yes | no) #IMPLIED
               LANGUAGE  (FR|EN)   #IMPLIED      >

<!-- ELEMENT : ORDER, ONLY FOR ORDER OF QU EC -->
<!ELEMENT ORDER  (%everyshort; , (IDENTIFICATION, (P|PARA)*, SIGNATURE) , %everyshort; ) >

<!-- ABBREV IS A UNIQUE IDENTIFIER FOR THE ORDER
     EX: ABBREV = "DGQ[999-AAAA][FR]" [TO BE DETERMINED....]   QU EC

-->

<!ATTLIST ORDER
               ABBREV    CDATA     #IMPLIED
               LANGUAGE  (FR|EN)   #IMPLIED    >



<!ELEMENT NOTEFINLR  ( %everylong; , (N?, TI?, (MARGINALNOTE?, P)+, STATHIST?) , %everylong;)>

<!ATTLIST NOTEFINLR
          ID              CDATA                    #IMPLIED
          NEXTID          CDATA                	   #IMPLIED
          PREVID          CDATA                	   #IMPLIED
          INFORCEDATE     CDATA                    #IMPLIED
          INFORCE         (O|V|F|FNK)              #IMPLIED
          CORRECTIONDATE  CDATA                    #IMPLIED
          MODDATE         CDATA                    #IMPLIED
          NOSHIPPING      CDATA                    #IMPLIED              >

<!--ELEMENT:  FOOTNOTE
    CLASS:    STRUCPUB-->
<!ELEMENT FOOTNOTE (N?, FREEFORM?) >

<!ATTLIST FOOTNOTE
          PRODUCT CDATA #IMPLIED   >

<!ELEMENT EDITNOTE (FREEFORM) >

<!ATTLIST EDITNOTE
          PRODUCT CDATA #IMPLIED
          OTHER   CDATA #IMPLIED >

<!--ELEMENT:  PATCH
    CLASS:    FUNCTION -->
<!ELEMENT PATCH ANY  >

<!ATTLIST PATCH
     REASON CDATA #REQUIRED  >

<!ELEMENT LEGIDDOC ( %everylong; , (CITE*, STATHIST?, FREEFORM?) , %everylong;)>

<!ATTLIST LEGIDDOC
          TIGRE           CDATA #IMPLIED
          DOCUMENT        (yes | no)  #IMPLIED
          ADDDATE         CDATA       #IMPLIED
          LANG            (EN | FR)    #IMPLIED         
          INITID          CDATA       #IMPLIED
          ID              CDATA                    #IMPLIED
          NEXTID          CDATA                	   #IMPLIED
          PREVID          CDATA                	   #IMPLIED
          INFORCEDATE     CDATA                    #IMPLIED
          INFORCE         (O|V|F|FNK)              #IMPLIED
          CORRECTIONDATE  CDATA                    #IMPLIED
          MODDATE         CDATA                    #IMPLIED
          NOSHIPPING      CDATA                    #IMPLIED              >


<!--ELEMENT:  RUNNING HEAD
    CLASS:    FUNCTION-->
<!ELEMENT RUNNING (#PCDATA) >

<!ATTLIST RUNNING
          PLACE CDATA #IMPLIED >


<!--ELEMENT:  PAGE FORMAT INFORMATION
    CLASS:    FUNCTION-->

<!ELEMENT FORMAT (RUNNING*) >


<!--ELEMENT:  CONVENTION
    CLASS:    STRUCLEG
    COMMENTS: NOW A DOCUMENT-->

<!ELEMENT CONVENT (N?, TI, (LONGTI?, CITE?, FREEFORM?),
                      (P | SECP)*, (DIVHEADING|FLOATHEADING|SEC)*,
                      (SCHEDULE | CONTAINR)*) >

<!ATTLIST CONVENT
                       DOCUMENT (yes | no)  #IMPLIED
                       ADDDATE CDATA       #IMPLIED
                       MODDATE CDATA       #IMPLIED
                       LANG    (EN | FR)  #IMPLIED
                       INITID  CDATA       #IMPLIED
                       ABBREV CDATA #IMPLIED
                                                        >


<!-- ELEMENT : EXTRACT OF A LAW OR A REGULATION-->
<!ELEMENT EXTRACT  ( %everylong; , (IDENTIFICATION,
                       (PREAMBLE|DIVHEADING|FLOATHEADING|SEC|EXTERNSCHEDULE|CONTAINR)*)
                                                                , %everylong;) >
<!-- ABBREV IS A UNIQUE IDENTIFIER FOR THE EXTRACT
     EX: ABBREV = "XLRQ[L-5.2][FR]"  QU EC
         ABBREV = "XRRQ[L-5.2R1][FR]"  QU EC

-->
<!ATTLIST EXTRACT
               ABBREV    CDATA     #IMPLIED
               LANGUAGE  (FR|EN)   #IMPLIED      >


<!-- ELEMENT : DEBUTBLOC  -->
<!ELEMENT 	DEBUTBLOC 		(#PCDATA)>

<!ATTLIST DEBUTBLOC
       OUVRAGE    	CDATA     #IMPLIED
       ENVOI    	CDATA     #IMPLIED
       PAGECOURANTE    	CDATA     #IMPLIED
       PAGEAVANT  	CDATA     #IMPLIED
       CESURE		(OUI|NON)   "NON"   >

<!-- ELEMENT : FINBLOC  -->
<!ELEMENT 	FINBLOC 		(#PCDATA)>

<!ATTLIST FINBLOC
       OUVRAGE    	CDATA     #IMPLIED
       ENVOI    	CDATA     #IMPLIED
       PAGECOURANTE    	CDATA     #IMPLIED
       PAGEAPRES  	CDATA     #IMPLIED
       CESURE		(OUI|NON)   "NON"   >


<!-- ELEMENT : IDENTIFICATION
     REPRESENT THE WORDING OF THE LAW OR THE REGULATION.

-->

<!ELEMENT IDENTIFICATION  ( %everylong; , (VOLUME?, LONGTI, SHORTTI, ALPHA?, RELATEDSTATUTE*, MODIFNOTE?,
                               READERNOTE?, STATHIST?) , %everylong;)>

<!ATTLIST IDENTIFICATION
          ID              CDATA                    #IMPLIED
          NEXTID          CDATA                	   #IMPLIED
          PREVID          CDATA                	   #IMPLIED
          INFORCEDATE     CDATA                    #IMPLIED
          INFORCE         (O|V|F|FNK)              #IMPLIED
          CORRECTIONDATE  CDATA                    #IMPLIED
          MODDATE         CDATA                    #IMPLIED
          NOSHIPPING      CDATA                    #IMPLIED              >


<!-- ELEMENT : ALPHA

     THE ALPHA REPRESENTS THE ALPHA OF THE LAW
     (EX: L-5.2).

-->
<!ELEMENT ALPHA  (#PCDATA) >

<!-- ELEMENT : RELATEDSTATUTE

     LAW(S) HABILITANTE(S)

 -->
<!ELEMENT RELATEDSTATUTE  (TI, CITE) >


<!-- ELEMENT : VOLUME

     TITLE OF THE PUBLICATION
-->
<!ELEMENT VOLUME  (%LINETXT;)* >

<!-- ELEMENT : SHORTTI

     SHORT TITLE OF PIECE OF LEGISLATION
-->
<!ELEMENT SHORTTI  (%LINETXT;)* >

<!--ELEMENT: LONG TITLE OF PIECE OF LEGISLATION
    CLASS:   GENPUB -->
<!ELEMENT LONGTI   (%LINETXT;)* >

<!-- ELEMENT : MODIFNOTE

     TEXT OF MODIFICATION.
-->
<!ELEMENT MODIFNOTE  (PT | PARA | (MARGINALNOTE?, SECP))* >
<!ATTLIST MODIFNOTE
          TYPENOTE CDATA #IMPLIED >

<!-- ELEMENT : MODIFTEXT

     TEXT OF A MODIFICATION FOUND AT THE SECTION LEVEL,
     OR A PARAGRAPH OR A SUB-PARAGRAPH.
     USED TO SPECIFIED THAT A SECTION OR A PARAGRAPH
     IS REPEALED, DELETED, ETC.
-->
<!ELEMENT MODIFTEXT  (%LINETXT;)* >

<!-- ELEMENT : READERNOTE

     NOTE TO READER USUALLY COMING FROM THE LEGISLATOR
     
-->
<!ELEMENT READERNOTE  (PT | PARA | (MARGINALNOTE?, SECP))* >
<!ATTLIST READERNOTE
          TYPENOTE CDATA #IMPLIED >


<!-- ELEMENT : PREAMBLE

     REPRESENTS THE PREAMBLE OF PIECE OF LEGISLATION.

-->
<!ELEMENT PREAMBLE  ( N?, TI?, (MARGINALNOTE?, P)+, STATHIST?)>
<!ATTLIST PREAMBLE
          ID              CDATA                    #IMPLIED
          NEXTID          CDATA                	   #IMPLIED
          PREVID          CDATA                	   #IMPLIED
          INFORCEDATE     CDATA                    #IMPLIED
          INFORCE         (O|V|F|FNK)              #IMPLIED
          CORRECTIONDATE  CDATA                    #IMPLIED
          MODDATE         CDATA                    #IMPLIED
          NOSHIPPING      CDATA                    #IMPLIED              >

<!-- ELEMENT : FLOATHEADING

     HEADING OF A FLOATHING TITLE. USUALLY IT IS A HEADING JUST ON TOP OF A SECTION.
-->
<!ELEMENT FLOATHEADING  ( %everylong; , ( N?, TI, SHORTTI?, MODIFNOTE?, STATHIST?) , %everylong; )>
<!ATTLIST FLOATHEADING
          STYLEAS         (PART|BOOK|TITLE|PRETITLE|
                           CHAP|DIV|SDIV|
                           SSDIV| SSSDIV|L1|L2|L3|L4) #IMPLIED
          CARSWELLELEMENT CDATA                    #IMPLIED
          LABEL           CDATA                    #IMPLIED
          ID              CDATA                    #IMPLIED
          NEXTID          CDATA                	   #IMPLIED
          PREVID          CDATA                	   #IMPLIED
          INFORCEDATE     CDATA                    #IMPLIED
          INFORCE         (O|V|F|FNK)              #IMPLIED
