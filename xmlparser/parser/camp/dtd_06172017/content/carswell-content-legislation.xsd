<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.2.2">
	<xs:include schemaLocation="mathml2/mathml2.xsd"/>
	<xs:redefine schemaLocation="carswell-content-common.xsd">
		<xs:complexType name="PrologDisplayType">
			<xs:complexContent>
				<xs:extension base="PrologDisplayType">
					<xs:group ref="CitationStatuteReferenceStatuteHistoryGroup" minOccurs="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="PrologDesignatorDisplayType">
			<xs:complexContent>
				<xs:extension base="PrologDesignatorDisplayType">
					<xs:group ref="CitationStatuteReferenceStatuteHistoryGroup" minOccurs="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="PrologTitleDisplayType">
			<xs:complexContent>
				<xs:extension base="PrologTitleDisplayType">
					<xs:group ref="CitationStatuteReferenceStatuteHistoryGroup" minOccurs="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="PrologDesignatorTitleDisplayType">
			<xs:complexContent>
				<xs:extension base="PrologDesignatorTitleDisplayType">
					<xs:group ref="CitationStatuteReferenceStatuteHistoryGroup" minOccurs="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="PrologLabelTitleDisplayType">
			<xs:complexContent>
				<xs:extension base="PrologLabelTitleDisplayType">
					<xs:group ref="CitationStatuteReferenceStatuteHistoryGroup" minOccurs="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="TermAndDefinitionsBodyType">
			<xs:complexContent>
				<xs:extension base="TermAndDefinitionsBodyType">
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:sequence>
							<xs:element ref="definition.legis"/>
							<xs:group ref="TermAndDefinitionsBodyGroup" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="EpilogDisplayType">
			<xs:complexContent>
				<xs:extension base="EpilogDisplayType">
					<xs:group ref="CitationStatuteReferenceStatuteHistoryGroup" minOccurs="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="QuoteLongBodyType">
			<xs:complexContent>
				<xs:extension base="QuoteLongBodyType">
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:group ref="QuoteLegisUnitGroup"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="GlossaryBodyType">
			<xs:complexContent>
				<xs:extension base="GlossaryBodyType">
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:group ref="GlossaryProposedGroup"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:complexType name="InlineType" mixed="true">
			<xs:complexContent mixed="true">
				<xs:extension base="InlineType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:redefine>
	<xs:complexType name="LegislationDivisionBodyType"/>
	<xs:complexType name="LegislationDivisionType">
		<xs:sequence>
			<xs:element name="prolog" type="PrologType" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="Base.Attributes"/>
		<xs:attributeGroup ref="LegislationVersionAttributes"/>
	</xs:complexType>
	<xs:complexType name="LegislationDivisionDesignatorType">
		<xs:sequence>
			<xs:element name="prolog" type="PrologDesignatorType"/>
		</xs:sequence>
		<xs:attributeGroup ref="Base.Attributes"/>
		<xs:attributeGroup ref="LegislationVersionAttributes"/>
	</xs:complexType>
	<xs:complexType name="LegislationDivisionTitleType">
		<xs:sequence>
			<xs:element name="prolog" type="PrologTitleType"/>
		</xs:sequence>
		<xs:attributeGroup ref="Base.Attributes"/>
		<xs:attributeGroup ref="LegislationVersionAttributes"/>
	</xs:complexType>
	<xs:complexType name="LegislationDivisionDesignatorTitleType">
		<xs:sequence>
			<xs:element name="prolog" type="PrologDesignatorTitleType"/>
		</xs:sequence>
		<xs:attributeGroup ref="Base.Attributes"/>
		<xs:attributeGroup ref="LegislationVersionAttributes"/>
	</xs:complexType>
	<xs:attributeGroup name="BaseAttributesLangRequiredGroup">
		<xs:attribute name="id" type="xs:string"/>
		<xs:attribute name="content.type" type="xs:string"/>
		<xs:attribute name="filter" type="xs:string"/>
		<xs:attribute name="tag.context" type="xs:string"/>
		<xs:attribute name="tag.was" type="xs:string"/>
		<xs:attribute name="lang" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="EN"/>
					<xs:enumeration value="FR"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="HistoricalRetentionAttributes">
		<xs:attribute name="historical" default="hide">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="show"/>
					<xs:enumeration value="hide"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="LegislationVersionAttributes">
		<xs:annotation>
			<xs:documentation>Attributes that control versionable elements</xs:documentation>
		</xs:annotation>
		<xs:attribute name="start.date" type="xs:date"/>
		<xs:attribute name="end.date" type="xs:date"/>
	</xs:attributeGroup>
	<xs:group name="QuoteLegisUnitGroup">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="legis.unit"/>
				<xs:group ref="LegislationDivisionAdditionalsGroup"/>
			</xs:choice>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="DivisionCommonGroup"/>
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:group name="GlossaryProposedGroup">
		<xs:sequence>
			<xs:element ref="proposed.legis"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="GlossaryContentGroup"/>
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:group name="LegislationDivisionAdditionalsGroup">
		<xs:choice>
			<xs:element ref="proposed.legis"/>
			<xs:element ref="application.legis"/>
			<xs:element ref="div.editorial.legis"/>
			<xs:element ref="div.generic.legis"/>
			<xs:element ref="technical.note"/>
			<xs:element ref="government.letter"/>
			<xs:element ref="comment"/>
			<xs:element ref="category.cross.reference"/>
			<xs:element ref="div.cross.reference"/>
			<xs:element ref="legis.quote"/>
			<xs:element ref="definition.legis"/>
			<xs:element ref="statute.history"/>
		</xs:choice>
	</xs:group>
	<xs:group name="LegislationDivisionCommonGroup">
		<xs:annotation>
			<xs:documentation>Grouping of common elements inside Legislative Divisions</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:group ref="DivisionCommonGroup"/>
			<xs:group ref="LegislationDivisionAdditionalsGroup"/>
		</xs:choice>
	</xs:group>
	<xs:group name="CitationStatuteReferenceStatuteHistoryGroup">
		<xs:annotation>
			<xs:documentation>Pairing of StatureReference and StatueHistory</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="citation" minOccurs="0"/>
			<xs:element ref="status.text" minOccurs="0"/>
			<xs:element ref="reference.statute" minOccurs="0"/>
			<xs:element ref="statute.history" minOccurs="0"/>
			<xs:element ref="scope" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="act">
		<xs:annotation>
			<xs:documentation>Legislative Act</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="prolog">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="PrologType">
								<xs:sequence minOccurs="0">
									<xs:element ref="parent.act" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="body">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="LegislationDivisionBodyType">
								<xs:sequence>
									<xs:choice minOccurs="0" maxOccurs="unbounded">
										<xs:group ref="LegislationDivisionCommonGroup"/>
										<xs:element ref="preamble"/>
										<xs:element ref="section"/>
										<xs:element ref="div.misc.legis"/>
										<xs:element ref="subdiv.legis"/>
										<xs:element ref="div.legis"/>
										<xs:element ref="part"/>
										<xs:element ref="chapter"/>
										<xs:element ref="title.legis"/>
										<xs:element ref="book"/>
										<xs:element ref="schedule"/>
										<xs:element ref="appendix"/>
										<xs:element ref="form"/>
									</xs:choice>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="epilog" type="EpilogType" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Base.Attributes"/>
			<xs:attributeGroup ref="LegislationVersionAttributes"/>
			<xs:attribute name="abbrev" type="xs:string"/>
			<xs:attribute name="legis.stage">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="bill"/>
						<xs:enumeration value="assent"/>
						<xs:enumeration value="proclaimed"/>
						<xs:enumeration value="not.proclaimed"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="regulation">
		<xs:annotation>
			<xs:documentation>Legislative Regulation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="prolog">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="PrologTitleType">
								<xs:sequence minOccurs="0">
									<xs:element ref="parent.act" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="body">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="LegislationDivisionBodyType">
								<xs:sequence>
									<xs:choice minOccurs="0" maxOccurs="unbounded">
										<xs:group ref="LegislationDivisionCommonGroup"/>
										<xs:element ref="preamble"/>
										<xs:element ref="section"/>
										<xs:element ref="rule"/>
										<xs:element ref="div.misc.legis"/>
										<xs:element ref="subdiv.legis"/>
										<xs:element ref="div.legis"/>
										<xs:element ref="part"/>
										<xs:element ref="chapter"/>
										<xs:element ref="title.legis"/>
										<xs:element ref="schedule"/>
										<xs:element ref="appendix"/>
										<xs:element ref="form"/>
									</xs:choice>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="epilog" type="EpilogType" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Base.Attributes"/>
			<xs:attributeGroup ref="LegislationVersionAttributes"/>
			<xs:attribute name="abbrev" type="xs:string"/>
			<xs:attribute name="legis.stage">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="bill"/>
						<xs:enumeration value="assent"/>
						<xs:enumeration value="proclaimed"/>
						<xs:enumeration value="not.proclaimed"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="regulation.type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="RULES"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
