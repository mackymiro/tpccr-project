  INITID CDATA  #IMPLIED">
<!-- Appear Everywhere Elements -->
<!ENTITY % every "EDITNOTE | FOOTNOTE | GLUE | PATCH | PROPOSED | QUOTE">
<!--Parameter Entity: Inline text Elements.
    Typographic conventions.
    Cross-references.
    Text exclusion/inclusion.
    Modification marker.
-->
<!-- <!ENTITY % linetxt "((#PCDATA) | bold | em | strike | under | 
                     sub | sup | 
                     textfrac | caseref | statref | term | termref | see | 
                     seealso | condtext | ex | text | modify)"> 
FOOTNOTE  EDITNOTE QUOTE added -->
<!ENTITY % linetxt "#PCDATA | BOLD | CONDTEXT | EM | EX | MODIFY | STRIKE | SUB | SUP |
  TERM | TERMREF | TEXT | TEXTFRAC | UNDER | SEE | SEEALSO |
  STATREF | CASEREF |
 %every;">
<!--Parameter Entity: In-line text elements + paragraph-breaking elements.
    Typographic conventions.
    Cross-references.
    Text exclusion/inclusion.
    Modification marker.
    Formulas.
    Graphic placeholders.
    Ordered and unordered lists.
    Quotes and excerpts.
    Tables.
    "Words and Phrases" term paragraphs.
-->

<!ENTITY % lritext "(( TABLE | (CASEREF | STATREF | REGREF | REFN | 
                       TERMP | CAPTION | AUTHORS)
                       |  (CASETRMT | STATTRMT)
                       |  ((TI | (CLICSOC?, CITE?)), 
                      ((VOLUME?, REPORTER, PAGENUM, COURTJUR?, DECIDER?) | 
                       (DECIDER?, COURTJUR, REPORTER, PAGENUM)))))" >

<!-- <!ENTITY % paratxt "((#PCDATA) | bold | em | strike | under | 
                     sub | sup | 
                     textfrac |
                     caseref | statref | term | termref | see | seealso |
                     condtext | ex | text |
                     modify |
                     df |
                     graphic |
                     item | line |
                     endnote |
                     quote | excerpt |
                     table |
                     termp )">     -->
<!ENTITY % paratxt "%linetxt; | ITEM | LINE | ENDNOTE | EXCERPT | DF | GRAPHIC | FILLABLEGRAPHIC | TABLE | TERMP">
<!--Parameter Entity: Formula and fraction elements.
    Typographic elements.
    Fraction.
    Fenced fraction.
-->
<!ENTITY % fractxt "#PCDATA | SUP | SUB | FRAC | FENCE | UNDER | %every;">
<!--Parameter Entity: Table Cell contents.
    author,surti,ti,subti,longti is for Title Pages. Moved from 
    bookiden floating elements.
    authtext deleted as obsolete: use line.
-->
<!ENTITY % celltxt "%linetxt; | GRAPHIC | FILLABLEGRAPHIC | FRAC | AUTHOR | SURTI | TI | SUBTI | LONGTI">
