<!ENTITY % car.ent PUBLIC "-//Carswell//ENTITIES Carswell//EN">
%car.ent;

<!ENTITY % linetxt "((#PCDATA) | bold | em |  strike | under | sub | sup | 
                     textfrac | caseref | statref | term | termref | see | 
                     seealso | condtext | ex | text | modify | link | internallink)" >

<!ENTITY % paratxt "((#PCDATA) | bold | em |  strike | under | sub | sup | 
                     textfrac |
                     caseref | statref | term | termref | see | seealso |
                     condtext | ex | text |
                     modify |
                     df |
                     graphic | fillablegraphic | link | internallink |                    
                     item | line |
                     endnote |
                     quote | excerpt |
                     table |
                     termp )" >     

<!ENTITY % fractxt "(#PCDATA) | sup | sub | frac | fence" >

<!ENTITY % celltxt "((%linetxt;) | graphic | fillablegraphic | frac |
                     author | surti | ti | subti | longti)" > 

<!ENTITY % product "product CDATA #IMPLIED">

<!ENTITY % sync "sync CDATA #IMPLIED">

<!ENTITY % tigre "tigre CDATA #IMPLIED">

<!ENTITY % decprop     "adddate CDATA #IMPLIED
                        moddate CDATA #IMPLIED">  

<!ENTITY % docprop    "document (yes | no)  #IMPLIED
                       adddate CDATA       #IMPLIED
                       moddate CDATA       #IMPLIED 
                       lang    (en | fr)    'en'
                       initid  CDATA       #IMPLIED"   >

<!ELEMENT root - - ( (( freeform |
                     act | reg | remision | misclaw | convent |
                     book | part | div |
                     legiddoc | sec | form | article | class | scheddoc |
                     decision | courtfil | casedige | sagdige |
                     essay | comment | casesdoc | appendoc | docp| 
                     preceden |
                     flentry | docwrapper |
                     polidoc |
                     tngroup |
                     bookiden | copyrigh | frontdoc | toc |
                     index | lrindex |
                     xrefdoc |
                     oDocPack | 
                     decdummy | abdigest | ruling | bcmvd | 
                     cmsset | termp |
                     keysubj | keyxref)* ,
                     (bookpart | frontmat | backmat | 
                     datapool | hub1 | root | glue)*))
      +(patch, editnote, footnote, quote, proposed, glue)>


<!-- ============   Document container:
                      chapters in a book,
                      Folio and On-line Table of Contents  ============ -->

<!ELEMENT bookpart - - (format? ,
                        n?, ti?, subti*,
                       (toc | frontdoc | 
                        freeform |
                        polidoc | form |
                        decision | decdummy | ruling | essay | courtfil |
                        tngroup | termp |
                        xrefdoc |
                        abdigest | casedige | bcmvd | sagdige |
                        flentry |
                        comment | casesdoc | docp | preceden | appendoc |
                        lrindex | index | 
                        keysubj | keyxref |  
                        oDocPack)*,
     (root | bookpart | act | reg | convent | remision | misclaw)*) >

<!ATTLIST bookpart
          abbrev   CDATA #IMPLIED
          label    CDATA #IMPLIED
          views    CDATA #IMPLIED
          fulln    CDATA #IMPLIED
          bookmark CDATA #IMPLIED >


<!-- ============         Case Law Decisions               ============ -->



<!ELEMENT decision - - (view*, ident, summarym, 
                       (opinion | appendix | statedis | solrecs | caseanno |
                        identbit | summabit)*) >
<!ATTLIST decision  
          tigre   CDATA     #IMPLIED  
          ccdbnum CDATA     #IMPLIED 
          decinum CDATA     #IMPLIED 
          %docprop;
          trackid CDATA     #IMPLIED 
          DB-PGNUM        CDATA           #IMPLIED>


<!ELEMENT view - - EMPTY >
<!ATTLIST view  
          name   CDATA #REQUIRED 
          number CDATA #REQUIRED >


<!ELEMENT ident - - (clicsoc, socs, statref*, cite?, correlas, othlang?,
                     digcites?, digcite* ,
                     courtjur, jurisdic? ,
                     decider, dateblk, year?, docket?, relaproc?, casehsts,
                     counsels, length?, refn?, modify?) >
<!ATTLIST ident 
          sync    CDATA #IMPLIED
          adddate CDATA #IMPLIED >

<!ELEMENT identbit - - (date | judgdate | heardate | causdate | courtjur |
                        jurisdic | decider | docket | casehist | counsels)* >


<!ELEMENT summabit - - (subject | caption | headnote | reflist | caseanno | 
                        casecomm | statepro | intromat)*  >


<!ELEMENT summarym - - (subject*, (caption | headnote | caseanno | reflist | 
                                   casesumm | text)*, 
                         casecomm*, statepro*, intromat?) >



<!ELEMENT classif - - (#PCDATA) >
<!ATTLIST classif
          index CDATA #IMPLIED >


<!ELEMENT classifn - - (#PCDATA) >
<!ATTLIST classifn
          type (canrep | abr | pid | crim) #IMPLIED
          index  CDATA                     #IMPLIED  
          prefix CDATA                     #IMPLIED >


<!ELEMENT clicsoc - - (%linetxt;)* >
<!ATTLIST clicsoc
          short CDATA #IMPLIED >



<!ELEMENT soc - - (%linetxt;)* >
<!ATTLIST soc      
          type (full | partial) #IMPLIED >


<!ELEMENT socs - - (soc*) >


<!ELEMENT solrec - - (%linetxt;)* >


<!ELEMENT solrecs - - (solrec*)  >



<!ELEMENT correlas - - (correlat*) >


<!ELEMENT correlat - - (soc?, cite, pagenum?, paranum?) >


<!ELEMENT counsel - - (%linetxt;)* >
<!ATTLIST counsel  
     label CDATA #IMPLIED   >


<!ELEMENT counsels - - (counsel*) >
<!ATTLIST counsels  
          label CDATA #IMPLIED   >


<!ELEMENT courtfil - - (n, ti, freeform, appendix*) >
<!ATTLIST courtfil 
          %docprop;
          label CDATA #IMPLIED >


<!ELEMENT courtjur - - (#PCDATA) >
<!ATTLIST courtjur  
          index   CDATA #IMPLIED 
          altform CDATA #IMPLIED
          DB-COURTCODE    CDATA     #IMPLIED>



<!ELEMENT decider - - (#PCDATA | sup)* >
<!ATTLIST decider  
          label CDATA #IMPLIED   >


<!ELEMENT cite - - (%linetxt;)* >
<!ATTLIST cite
          product CDATA #IMPLIED
          type (reported | unrep | electrc | digest | issue | 
                tracking | neutral) #IMPLIED
          index   CDATA #IMPLIED
          citenum CDATA #IMPLIED
          views   CDATA #IMPLIED 
          DB-YEAR        CDATA                     #IMPLIED
          DB-VOl         CDATA                     #IMPLIED
          DB-REPCODE     CDATA                     #IMPLIED
          DB-PAGE        CDATA                     #IMPLIED>




<!ELEMENT othlang - - ((#PCDATA) | caseref)* >


<!ELEMENT judgdate - - (%linetxt;)* >
<!ATTLIST judgdate  
          type     (oral | written) #IMPLIED 
          yyyymmdd CDATA           #IMPLIED  
          label    CDATA           #IMPLIED  
          index    CDATA           #IMPLIED >


<!ELEMENT causdate - - (%linetxt;)* >
<!ATTLIST causdate 
          label CDATA #IMPLIED >



<!ELEMENT jurisdic - - (%linetxt;)* >
<!ATTLIST jurisdic  
          label CDATA #IMPLIED
          DB-JURCODE      CDATA                  #IMPLIED >



<!ELEMENT digcite - - (#PCDATA) >
<!ATTLIST digcite  
          views CDATA #IMPLIED  
          index CDATA #IMPLIED >


<!ELEMENT digcites - - (digcite*) >


<!ELEMENT dateblk - - (judgdate | heardate | causdate | date)* >


<!ELEMENT date - - (#PCDATA) >
<!ATTLIST date  
          yyyymmdd CDATA #IMPLIED
          label    CDATA #IMPLIED 
          DB-TYPE      CDATA                        #IMPLIED
          DB-DESC      CDATA                        #IMPLIED>




<!ELEMENT headnblk - - (freeform) >


<!ELEMENT headnote - - (headnblk? | (fact*, held*, reason*)) >
<!ATTLIST headnote  
          type (full | digest)  #IMPLIED
          views CDATA          #IMPLIED  
          lang (en | fr)       #IMPLIED >


<!ELEMENT fact - - (p*, block4*) >
<!ATTLIST fact  
          views CDATA #IMPLIED  >


<!ELEMENT held - - (p*) >
<!ATTLIST held  
          views CDATA #IMPLIED  
          label CDATA #IMPLIED >


<!ELEMENT reason - - (p*, block4*)  >
<!ATTLIST reason 
          views CDATA #IMPLIED  >



<!ELEMENT reflist - - (freeform) >
<!ATTLIST reflist  
          type (statutes | rules | regs | treaties | tariffs | authorit | 
                cases | words | forms | rulings | policies) #IMPLIED 
          lang (en | fr)                                #IMPLIED  >



<!ELEMENT termp   - - (((term, (termalt* | seealso*),
                          (
                          (jurisdic, quote, subjtitl+, caseref*, decider) |
                          (quote, subjtitl*, caseref?, decider?) | (caseref)
                          )
                          ) |
                          (jurisdic?, quote, subjtitl+, caseref, decider))) >
<!ATTLIST termp %docprop; >


<!ELEMENT heardate - - (%linetxt;)* >
<!ATTLIST heardate  
          yyyymmdd CDATA #IMPLIED
          label    CDATA #IMPLIED   >


<!ELEMENT year - - (#PCDATA) >
<!ATTLIST year  
          index CDATA #IMPLIED >


<!ELEMENT docket - - (%linetxt;)* >
<!ATTLIST docket  
          label CDATA #IMPLIED   >


<!ELEMENT relaproc - - ((#PCDATA) | caseref)* >


<!ELEMENT casehist - - (casedisp*, caseref) > 
<!ATTLIST casehist
          type (prior|later) #IMPLIED
          views CDATA #IMPLIED >


<!ELEMENT casehsts - - (casehist*) >


<!ELEMENT subject - - (#PCDATA)  >
<!ATTLIST subject  
          type (canrep | abr | pid | crim) #IMPLIED 
          index CDATA                   #IMPLIED >


<!ELEMENT subjtitl - - (#PCDATA) >
<!ATTLIST subjtitl  
          type (abr | pid | crim) #IMPLIED  
          index CDATA           #IMPLIED >



<!ELEMENT capphra - - (%linetxt;)* >
<!ATTLIST capphra
          label CDATA #IMPLIED >


<!ELEMENT caption - - ((subject? | subjtitl?), classifn?, classif*, capphra*) >
<!ATTLIST caption
          views CDATA #IMPLIED
          main  CDATA #IMPLIED
          type  (canrep | abr | pre-abr | pid | crim | tax | rfl | 
                 weekly) #IMPLIED
          lang  (en | fr) #IMPLIED >


<!ELEMENT caseanno - - (freeform, authors?, line*) >
<!ATTLIST caseanno
          label CDATA #IMPLIED
          views CDATA #IMPLIED >


<!ELEMENT statedis - - (%linetxt;)* >
 <!ATTLIST statedis 
          lang  (en | fr) #IMPLIED  
          views CDATA     #IMPLIED  >


<!ELEMENT statepro - - (%paratxt;)* >
<!ATTLIST statepro 
          lang (en | fr) #IMPLIED  
          views CDATA    #IMPLIED  >


<!ELEMENT intromat - - (freeform*) >


<!ELEMENT opinion - - (block2?, opintro?, freeform?) >
<!ATTLIST opinion 
          views CDATA #IMPLIED >


<!ELEMENT opintro - - (#PCDATA | decider | per | caseref)* >

  

<!ELEMENT np - - (n, p*) >
<!ATTLIST np  
          product CDATA #IMPLIED  
          sync    CDATA #IMPLIED >

 
<!ELEMENT bold - - (%linetxt;)* >


<!ELEMENT under - - (%linetxt;)* > 



<!-- ============              Legislation               ============ -->


<!ELEMENT act     - - (ti, legiddoc,
                       sec*, prepart*, (mdiv | mdiv0)*, sdiv*, div*,
                      (part* | chapter*), title*, book*,
                      (schedule | form | containr)*,
                       stathist*)>
<!ATTLIST act
          abbrev   CDATA      #IMPLIED
          proclaim (yes | no) #IMPLIED>  



<!ELEMENT reg - - (n?, ti, legiddoc, 
                   sec*, rule*, prepart*, (mdiv | mdiv0)*, sdiv*, div*,
                   (part* | chapter*), title*, 
                   (schedule | containr | form)*) >
<!ATTLIST reg  
          abbrev CDATA   #IMPLIED
          proclaim (yes | no) #IMPLIED  
          label  CDATA   #IMPLIED  
          type   (rules) #IMPLIED >



<!ELEMENT actunder - - (%linetxt;)* >
<!ATTLIST actunder
          label  CDATA   #IMPLIED  >


<!ELEMENT remision - - ( n?, ti, legiddoc?, 

                        ( 
                          (freeform, appendix*) |

                          (sec*, (mdiv | mdiv0)*, sdiv*, div*, part*, 
                          (schedule | appendix | containr)*, form*)  
                        ) )>
<!ATTLIST remision  
          %docprop; 

          abbrev CDATA #IMPLIED >



<!ELEMENT convent - - (n?, ti, ((longti?, cite?, freeform?) | legiddoc),
                      (p | secp)*, (sec* | article*), 
                      ((mdiv* | mdiv0*) | part* | chapter*),  
                      (schedule | appendix | containr)*) >
<!ATTLIST convent
          %docprop; 

          abbrev CDATA #IMPLIED >


<!ELEMENT misclaw - - (n?, ti, ((longti?, freeform?) | legiddoc), 
                       sec*, (mdiv | mdiv0)*, article*, rule*, 
                      (part* | chapter*),
                      (schedule | appendix | containr)* ,
                       form*, stathist?) >
<!ATTLIST misclaw  
          abbrev CDATA #IMPLIED  
          label  CDATA #IMPLIED >



<!ELEMENT rule - - (n, ti?, (secp | sec)*, (mdiv | mdiv2)*, 
                       (schedule | containr)*, stathist?) >
<!ATTLIST rule  
          label CDATA "Rule"  >



<!ELEMENT legiddoc - - (actunder*, longti?, cite*, citeinfo?, 
                        stathist?, freeform?, preamble?) >
<!ATTLIST legiddoc 
          tigre CDATA #IMPLIED  
          %docprop; >


<!ELEMENT citeinfo - - (#PCDATA)* >


<!ELEMENT longti - - (%linetxt;)* >

<!ELEMENT preamble - - (freeform) >

<!ELEMENT mdiv   - - (n?, ti, sec*, rule*, mdiv*, article*, form*,
                     (schedule | appendix | containr)*, stathist?) > 
<!ATTLIST mdiv  
          label CDATA #IMPLIED >

<!ELEMENT mdiv0 - - (n?, ti, sec*, rule*, mdiv1*, article*, form* ,
                     (schedule | containr)*, stathist?) >
<!ATTLIST mdiv0  
          label CDATA #IMPLIED >


<!ELEMENT mdiv1 - - (n?, ti, sec*, rule*, mdiv2*, article*, form*, 
                     (schedule | containr)*, stathist?) >
<!ATTLIST mdiv1  
          label CDATA #IMPLIED   >


<!ELEMENT mdiv2 - - (n?, ti, sec*, rule*, mdiv3*, article*, form*, 
                     (schedule | containr)*, stathist?) >
<!ATTLIST mdiv2  
          label CDATA #IMPLIED  >


<!ELEMENT mdiv3 - - (n?, ti, sec*, rule*, mdiv4*, article*, form*, 
                    (schedule | containr)*, stathist?) >
<!ATTLIST mdiv3  
          label CDATA #IMPLIED  >


<!ELEMENT mdiv4 - - (n?, ti, sec*, article*, form*,
                    (schedule | containr)*, stathist?) >
<!ATTLIST mdiv4  
          label CDATA #IMPLIED  >


<!ELEMENT part - - (n, ti?, freeform?, 
                    ((sec*, form*, rule*, (mdiv | mdiv1)*, article*, 
                       sdiv*, div*, book*, 
                       (schedule | appendix | containr)*) | 
                       (chapter)+ | (title)+), stathist?, root?) >
<!ATTLIST part  
          label CDATA "Part" >


<!ELEMENT book     - - (n, ti, sec*, chapter*, title*, stathist?, root?) >
<!ATTLIST book  
          label CDATA "Book" > 


<!ELEMENT title - - (n, ti?, sec*, mdiv1*, div*, chapter*, stathist?) > 
<!ATTLIST title  
          label CDATA "Title" >



<!ELEMENT chapter - - (n, ti, sec*, article*, 
                      (mdiv | mdiv0 | mdiv1)*, div*, stathist?) >
<!ATTLIST chapter
          label CDATA #IMPLIED >


<!ELEMENT div - - (n, ti, freeform?, sec*, article*, rule*, (mdiv | mdiv1)*,
                   sdiv*, (schedule | containr)*, stathist?, root?) > 
<!ATTLIST div  
          label CDATA "Division" >



<!ELEMENT sdiv - - (n, ti, sec*, (mdiv | mdiv1)*, ssdiv*, root?) >
<!ATTLIST sdiv  
          label CDATA "Subdivision" >


<!ELEMENT containr - - (scheddoc | form)* >
<!ATTLIST containr
          label CDATA #REQUIRED >


<!ELEMENT scheddoc - - (n, ti?, statref?, freeform, appendix*, stathist?) >
<!ATTLIST scheddoc 
          label     CDATA "Schedule" 
          tigre     CDATA #IMPLIED     
          %docprop;
          product   CDATA #IMPLIED >


<!ELEMENT schedule - - (n?, ti?, statref?, secp?, 
                        ((sec*, (mdiv | mdiv0)*, sdiv*, div*, part*) |
                          freeform | form* | class* | convent*)*, 
                          stathist?) >
<!ATTLIST schedule 
          label    CDATA "Schedule"
          tigre    CDATA #IMPLIED     
          %docprop;
          product  CDATA #IMPLIED   >


<!ELEMENT article - - (n, ti?, (secp | sec)+)>
<!ATTLIST article %docprop;
          label    CDATA #IMPLIED >



<!ELEMENT class - - (fulln?, n, ti?, statref?, (sec+ | secp+), stathist?) >
<!ATTLIST class 
          label CDATA #IMPLIED
          %docprop; >


<!ELEMENT sec - - (fulln?, n, oldn?, ti?, 
                   ((secp, (ti?, secp)*) | (secdiv | ssec)+ | para+),
                     (schedule* | appendix*), form*, stathist?, archive?) >
<!ATTLIST sec  
          product  CDATA #IMPLIED  
          tigre    CDATA #IMPLIED  
          sync     CDATA #IMPLIED
          %docprop;
          id       CDATA #IMPLIED >


<!ELEMENT sssdiv - - (n, ti, sec+) >
<!ATTLIST sssdiv 
          label CDATA #IMPLIED >


<!ELEMENT secdiv - - (ti, ssec+) >


<!ELEMENT secp - - ((%paratxt;) | vardef | para | def | ssec)* >
<!ATTLIST secp 
          id CDATA #IMPLIED >
     

<!ELEMENT ssdiv - - (n, ti, (sec+ | sssdiv+)) >
<!ATTLIST ssdiv  
          label CDATA "Sub-subdivision"   >


<!ELEMENT ssec - - (fulln?, n, ti?, ((ssecp, (ti?, ssecp)*) | para*),
                     stathist?) >
<!ATTLIST ssec  
          product CDATA #IMPLIED  
          sync CDATA    #IMPLIED >


<!ELEMENT ssecp - - ((%paratxt;) | p | vardef | para | def)* >


<!ELEMENT def - - (fulln?, ti?, defp+, stathist?) >
<!ATTLIST def  
          product CDATA #IMPLIED  
          sync    CDATA #IMPLIED >

<!ELEMENT defp - - ((%paratxt;) | p | vardef | ssec | para)* >



<!ELEMENT ssscl - - (fulln?, n, sssclp) >


<!ELEMENT sssclp - - ((%paratxt;) | p)* >


<!ELEMENT para - - (fulln?, n, ti?, (parap+ | spara+), stathist?) >
<!ATTLIST para  
     product CDATA #IMPLIED  
     sync CDATA #IMPLIED >


<!ELEMENT parap - - ((%paratxt;) | p | varpara | spara | def)* >



<!ELEMENT spara - - (fulln?, n, ti?, (sparap | cl+), stathist?) >


<!ELEMENT sparap - - ((%paratxt;) | p | varspara | cl)* >


<!ELEMENT cl - - (fulln?, n, ti?, (clp | scl+), stathist?) >


<!ELEMENT clp - - ((%paratxt;) | p | varcl | scl)* >


<!ELEMENT scl - - (fulln?, n,  ti?, (sclp | sscl+))  >


<!ELEMENT sclp - - ((%paratxt;) | p | varscl | sscl)* >


<!ELEMENT sscl - - (fulln?, n, (ssclp | ssscl+)) >


<!ELEMENT ssclp - - ((%paratxt;) | p | varsscl | ssscl)* >




<!ELEMENT var - - (%linetxt;)*  >


<!ELEMENT varcl - - (var, clp, stathist?) >


<!ELEMENT vardef - - (var, defp, stathist?) >


<!ELEMENT varpara - - (var, parap, stathist?) >


<!ELEMENT varscl - - (var, sclp, stathist?) >


<!ELEMENT varspara - - (var, sparap, stathist?) >


<!ELEMENT varsscl - - (var, ssclp, stathist?) >


<!ELEMENT form - - (n?, oldn?, ti?, statref?, freeform?, 
                    (appendix* | schedule*), stathist?) >
<!ATTLIST form  
          tigre CDATA #IMPLIED  
          label CDATA #IMPLIED  
          product CDATA #IMPLIED  
          %docprop; >


<!ELEMENT fulln - - (%linetxt;)* >


<!ELEMENT oldn - - (%linetxt;)* >
<!ATTLIST oldn
          product CDATA #IMPLIED >


<!ELEMENT proposed - - (ti, (quote | applic)*, 
                        (technote | xrefcat | comment)*) >
<!ATTLIST proposed  
     effect (amend | add | repeal | other) #IMPLIED  
     stage  (bill, assent)              #IMPLIED
     other   CDATA                      #IMPLIED
     product CDATA                      #IMPLIED  >


<!ELEMENT applic   - - (ti, freeform) > 



<!-- ============           Policy Documents               ============ -->


<!ELEMENT polidoc - - (polident, polisumm?, freeform, appendix*)  >
